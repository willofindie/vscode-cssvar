{
  "name": "cssvar",
  "displayName": "CSS Var Complete",
  "keywords": [
    "css",
    "css3",
    "variable",
    "css variable",
    "css var",
    "cssvar",
    "autocomplete",
    "complete",
    "intellisense"
  ],
  "description": "Intellisense support for CSS Variables",
  "version": "0.2.0",
  "publisher": "phoenisx",
  "license": "MIT",
  "homepage": "https://github.com/willofindie/vscode-cssvar",
  "bugs": {
    "url": "https://github.com/willofindie/vscode-cssvar/issues"
  },
  "repository": {
    "url": "https://github.com/willofindie/vscode-cssvar",
    "type": "git"
  },
  "icon": "img/icon.png",
  "engines": {
    "vscode": "^1.46.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:css",
    "onLanguage:scss",
    "onLanguage:sass",
    "onLanguage:less",
    "onLanguage:javascript",
    "onLanguage:javascriptreact",
    "onLanguage:typescript",
    "onLanguage:typescriptreact"
  ],
  "main": "./out/extension.js",
  "preview": false,
  "contributes": {
    "configuration": {
      "title": "CSS Var Complete",
      "properties": {
        "cssvar.files": {
          "type": [
            "array",
            "object",
            "null"
          ],
          "default": null,
          "description": "CSS variable file relative/absolute paths to include for the Intellisense"
        },
        "cssvar.extensions": {
          "type": [
            "array",
            "null"
          ],
          "default": null,
          "description": "File extensions to include"
        },
        "cssvar.themes": {
          "type": [
            "array",
            "null"
          ],
          "default": null,
          "description": "CSS Theme classname list"
        },
        "cssvar.excludeThemedVariables": {
          "type": "boolean",
          "default": false,
          "description": "Exclude themed variables"
        },
        "cssvar.disableSort": {
          "type": "boolean",
          "default": false,
          "description": "Disables default sorting applied by VSCode"
        }
      }
    }
  },
  "scripts": {
    "postinstall": "husky install",
    "vscode:prepublish": "yarn run build",
    "pub": "vsce publish --yarn",
    "prebuild": "rimraf out",
    "build": "rollup -c --environment INCLUDE_DEPS,BUILD,NODE_ENV:production",
    "watch": "rollup -c -m -w",
    "lint": "eslint src --ext ts",
    "lint:fix": "eslint src --ext ts --fix",
    "test": "cross-env NODE_ENV=production jest"
  },
  "devDependencies": {
    "@babel/core": "^7.13.15",
    "@babel/preset-env": "^7.13.15",
    "@babel/preset-typescript": "^7.13.0",
    "@rollup/plugin-commonjs": "^18.0.0",
    "@rollup/plugin-node-resolve": "^11.2.1",
    "@types/css": "^0.0.31",
    "@types/glob": "^7.1.3",
    "@types/jest": "^26.0.22",
    "@types/lodash": "^4.14.168",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.46.0",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "babel-jest": "^26.6.3",
    "cross-env": "^7.0.3",
    "esbuild": "^0.10.2",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-jest": "^24.4.0",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^5.2.0",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "lodash": "^4.17.21",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.43.0",
    "rollup-plugin-esbuild": "^3.0.2",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "fast-glob": "^3.2.5",
    "polished": "^4.1.1",
    "postcss": "^8.2.8"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "npm run lint:fix"
    ]
  }
}
